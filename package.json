{
	"name": "demo-content-for-blocks",
	"version": "1.2.0",
	"description": "Demo Content for Blocks WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/demo-content-for-blocks/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"dev": "webpack --mode development --watch --config ./webpack/build.config.babel.js",
		"prod": "webpack --mode production --config ./webpack/build.config.babel.js",
		"precopy": "rm -rf _release",
		"copy": "webpack --mode production --config ./webpack/copy.config.babel.js",
		"postcopy": "rm _release/_temp.js",
		"pragma": "webpack --mode production --config ./webpack/pragma.config.babel.js",
		"postpragma": "rm _release/inc/_temp.js",
		"version": "webpack --mode production --config ./webpack/version.config.babel.js",
		"postversion": "rm _temp.js",
		"prebuild": "npm run type-check && npm run lint && npm run prettier && npm run version",
		"build": "npm run prod",
		"postbuild": "npm run copy && npm run pragma",
		"type-check": "tsc",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"prettier": "prettier --check 'src/**/*.{ts,tsx}'",
		"start": "npm run dev"
	},
	"dependencies": {
		"react-tiny-popover": "^4.0.0",
		"tinycolor2": "^1.4.1",
		"uuid": "^7.0.2"
	},
	"devDependencies": {
		"@babel/core": "^7.8.7",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/preset-env": "^7.8.7",
		"@babel/preset-react": "^7.8.3",
		"@babel/preset-typescript": "^7.8.3",
		"@babel/register": "^7.8.6",
		"@types/lodash": "^4.14.149",
		"@types/react": "^16.9.23",
		"@types/tinycolor2": "^1.4.2",
		"@types/uuid": "^7.0.2",
		"@types/wordpress__api-fetch": "^3.2.2",
		"@types/wordpress__blocks": "^6.4.5",
		"@types/wordpress__components": "^8.5.2",
		"@types/wordpress__compose": "^3.4.0",
		"@types/wordpress__data": "^4.6.6",
		"@types/wordpress__edit-post": "^3.5.1",
		"@types/wordpress__element": "^2.4.0",
		"@types/wordpress__hooks": "^2.4.0",
		"@types/wordpress__i18n": "^3.4.0",
		"@types/wordpress__plugins": "^2.3.4",
		"@types/wordpress__url": "^2.3.0",
		"@typescript-eslint/eslint-plugin": "^2.24.0",
		"@typescript-eslint/parser": "^2.24.0",
		"babel-loader": "^8.0.6",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.4.2",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.19.0",
		"file-loader": "^6.0.0",
		"mini-css-extract-plugin": "^0.9.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"prettier": "^1.19.1",
		"string-replace-loader": "^2.2.0",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.8.3",
		"webpack": "^4.42.0",
		"webpack-cli": "^3.3.11",
		"webpack-loader-clean-pragma": "^1.0.2"
	}
}
