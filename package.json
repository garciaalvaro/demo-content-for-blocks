{
	"name": "demo-content-for-blocks",
	"version": "1.1.1",
	"description": "Demo Content for Blocks WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/demo-content-for-blocks/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"dev": "webpack --mode development --watch --config ./.webpack/build.config.babel.js",
		"prod": "webpack --mode production --config ./.webpack/build.config.babel.js",
		"copy": "webpack --mode production --config ./.webpack/copy.config.babel.js && rm _release/_temp.js",
		"version": "webpack --mode production --config ./.webpack/version.config.babel.js && rm _temp.js",
		"pragma": "webpack --mode production --config ./.webpack/pragma.config.babel.js && rm _release/inc/_temp.js",
		"release": "rm -rf build _release && npm run type && npm run version && npm run prod && npm run copy && npm run pragma",
		"type": "tsc"
	},
	"dependencies": {
		"tinycolor2": "^1.4.1",
		"uuid": "^3.3.3"
	},
	"devDependencies": {
		"@babel/core": "^7.6.0",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.5",
		"@babel/preset-env": "^7.6.0",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.6.0",
		"@babel/register": "^7.6.0",
		"@types/lodash": "^4.14.138",
		"@types/node": "^12.7.5",
		"@types/react": "^16.9.2",
		"@types/react-dom": "^16.9.0",
		"@types/tinycolor2": "^1.4.2",
		"@types/uuid": "^3.4.5",
		"@types/wordpress__api-fetch": "^3.2.2",
		"@types/wordpress__blocks": "^6.4.3",
		"@types/wordpress__components": "^7.4.4",
		"@types/wordpress__compose": "^3.4.0",
		"@types/wordpress__data": "^4.6.2",
		"@types/wordpress__edit-post": "^3.5.0",
		"@types/wordpress__element": "^2.4.0",
		"@types/wordpress__hooks": "^2.4.0",
		"@types/wordpress__i18n": "^3.4.0",
		"@types/wordpress__plugins": "^2.3.3",
		"@types/wordpress__url": "^2.3.0",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.4",
		"css-loader": "^3.2.0",
		"file-loader": "^4.2.0",
		"lodash": "^4.17.15",
